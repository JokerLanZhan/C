#define _CRT_SECURE_NO_WARNINGS 1

#include <stdio.h>

int main()
{
	int ch = 0;
	//ctrl + z
	//EOF - end of file -> -1
	while((ch = getchar()) != EOF)
	{
		putchar(ch);
	}

	//int ch = getchar();
	//putchar(ch);
	//printf("%c\n", ch);
	return 0;
}



